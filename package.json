{
  "name": "@unlockdep/app-template-react-typescript-sass",
  "description": "A preconfigured template for Snowpack with React Typescript and Sass",
  "version": "1.0.0",
  "license": "MIT",
  "bugs": "https://github.com/UnlockDep/app-template-react-typescript-sass/issues",
  "homepage": "https://github.com/UnlockDep/app-template-react-typescript-sass#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/UnlockDep/app-template-react-typescript-sass.git"
  },
  "keywords": [
    "csa-template",
    "snowpack"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/web-test-runner-plugin": "^0.1.5",
    "@testing-library/react": "^11.2.7",
    "@types/chai": "^4.2.19",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/snowpack-env": "^2.3.3",
    "@web/test-runner": "^0.10.2",
    "chai": "^4.3.4",
    "prettier": "^2.3.1",
    "snowpack": "^2.18.5",
    "typescript": "^4.3.4"
  }
}
